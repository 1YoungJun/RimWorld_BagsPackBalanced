<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Sack -->
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_Sack</defName>
		<label>sack</label>
		<description>A sack, a large bag used for carrying various items, is made from durable materials to easily transport heavy loads.</description>
		<graphicData>
			<texPath>Things/Sack/Sack</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<costStuffCount>50</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<statBases>
			<Insulation_Cold>2</Insulation_Cold>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>5000</WorkToMake>
			<Mass>0.5</Mass>
			<EquipDelay>1</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<CarryingCapacity>10</CarryingCapacity>
			<MoveSpeed>-0.3</MoveSpeed>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelMisc</li>
		</thingCategories>
		<apparel>
			<bodyPartGroups>
				<li>Waist</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Sack/Sack</wornGraphicPath>
			<useWornGraphicMask>false</useWornGraphicMask>
			<layers>
			   <li>Back</li>
			</layers>
			<tags>
				<li>Neolithic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>
		<tradeTags>
			<li>BasicClothing</li>
		</tradeTags>
	</ThingDef>
	
